drop table meta;
create table meta ( config json );
insert into meta ( config ) values ( json('{
	"layer": {
		"wayareaconflict": {
			"geometry":	"geometry",
			"srid":		"4326",
			"columns":	[ "lineway_id", "lineway_type", "lineway_changeset",
					"areaway_id", "areaway_type", "areaway_changeset" ],
			"popup":	"<a href=\"https://www.openstreetmap.org/way/{{ lineway_id }}\">Way {{ lineway_id }} {{ lineway_type }}</a><br><a href=\"https://www.openstreetmap.org/way/{{ areaway_id }}\">Area {{ areaway_id }} {{ areaway_type }}</a><br>{{> remotecontrol }}"
		}
	}
}'));
